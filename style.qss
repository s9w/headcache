QLineEdit{
	background-color: #e6e6e6;
	border-radius: 1px;
}

#mainframe, #editor, #preview, #file_list, #part_list{
	background-color: #222222;
	color: #f8f8f2;
}

QListWidget{
	border: 1px solid gray;
}

QListWidget::item{
