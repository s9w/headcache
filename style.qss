}

QListWidget::item:selected{
    background-color: #444444;
}

#editor{
	border: 1px solid gray;
}

#preview{
	border: 1px solid gray;
}

#search_result_list{
    background-color: #222222;
}

#search_result_list::item{
    background-color: #222222;
}

#search_result_list::item:selected{
    background-color: #444444;
}

#inner_label{
    color: #f8f8f2;
}